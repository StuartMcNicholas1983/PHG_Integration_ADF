					},
					"parameters": {
						"PipelineName": "[parameters('TRG_TW_7am_to_5pm_15Mins_properties_pipeline_parameters_PipelineName')]",
						"SequenceList": "[parameters('TRG_TW_7am_to_5pm_15Mins_properties_pipeline_parameters_SequenceList')]",
						"LoadType": "[parameters('TRG_TW_7am_to_5pm_15Mins_properties_pipeline_parameters_LoadType')]"
					}
				},
				"type": "TumblingWindowTrigger",
				"typeProperties": {
					"frequency": "Minute",
					"interval": 15,
					"startTime": "2023-08-07T09:07:00Z",
					"delay": "00:00:00",
					"maxConcurrency": 50,
					"retryPolicy": {
						"intervalInSeconds": 30
					},
