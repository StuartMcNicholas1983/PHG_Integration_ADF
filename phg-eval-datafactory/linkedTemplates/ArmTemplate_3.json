{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "phg-eval-datafactory"
		},
		"TRG_TW_7am_to_5pm_15Mins_properties_pipeline_parameters_PipelineName": {
			"type": "string",
			"defaultValue": "OH_D365_DataSync"
		},
		"TRG_TW_7am_to_5pm_15Mins_properties_pipeline_parameters_SequenceList": {
			"type": "string",
			"defaultValue": "[[C],[T]"
		},
		"TRG_TW_7am_to_5pm_15Mins_properties_pipeline_parameters_LoadType": {
			"type": "string",
			"defaultValue": "Incremental"
		},
		"TRG_TW_7am_to_5pm_60Mins_properties_PL_Control_parameters_PipelineName": {
			"type": "string",
			"defaultValue": "OH_D365_DataSync"
		},
		"TRG_TW_7am_to_5pm_60Mins_properties_PL_Control_parameters_SequenceList": {
			"type": "string",
			"defaultValue": "[[C],[T]"
		},
		"TRG_TW_7am_to_5pm_60Mins_properties_PL_Control_parameters_LoadType": {
			"type": "string",
			"defaultValue": "Incremental"
		}
