							"SequenceList": "[parameters('TRG_TW_7am_to_5pm_60Mins_properties_PL_Control_parameters_SequenceList')]",
							"LoadType": "[parameters('TRG_TW_7am_to_5pm_60Mins_properties_PL_Control_parameters_LoadType')]"
