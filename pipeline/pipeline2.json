{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Copy data2",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_SQL_OnPrem_Tbl",
						"type": "DatasetReference",
						"parameters": {
							"ServerName": "BBP-PRO2SQL-01\\PRO2SQLTEST",
							"DatabaseName": "OH_D365_Config",
							"SchemaName": "d365",
							"TableName": "stg_contact"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_Datalake_PipeDelimited",
						"type": "DatasetReference",
						"parameters": {
							"Container": "data-sync",
							"Directory": "OH_D365_DataSync/Staging/Fortis",
							"Filename": "fortis_contact.csv"
						}
					}
				]
			}
		],
		"annotations": []
	}
}